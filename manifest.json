{
  "manifest_version": 2,
  "name": "Tabulator",
  "version": "0.5.0",

  "description": "Like OneTab but leaner and FOSS",
  "icons": {},

  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Tabulator"
  },

  "background": {
    "page": "tabulator.html",
    "persistent": false
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "contextMenus"
  ]
}
